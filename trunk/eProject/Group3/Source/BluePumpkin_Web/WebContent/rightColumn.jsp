<div class="module">
	<div>
		<div>
			<div>
				<%

				
				%>	
				<h3>Welcome</h3>
				<% 

				%>	
				
				
				<h3>Login Form</h3>
					<form action="Login" method="post" name="login" id="form-login" >
						<fieldset class="input">
							<p id="form-login-username">
								<label for="modlgn_username">Username</label><br />
								<input id="modlgn_username" type="text" name="txtUser" class="inputbox" alt="username" size="18" />
							</p>
							<p id="form-login-password">
								<label for="modlgn_passwd">Password</label><br />
								<input id="modlgn_passwd" type="password" name="txtPass" class="inputbox" size="18" alt="password" />
							</p>
							<p id="form-login-remember">
								<label for="modlgn_remember">Remember Me</label>
								<input id="modlgn_remember" type="checkbox" name="remember" class="inputbox" value="yes" alt="Remember Me" />
							</p>
								<input type="submit" name="Action" class="button" value="Login" />
						</fieldset>
						<ul>
							<li>
								<a href="#">
								Forgot your password?</a>
							</li>
							<li>
								<a href="#">
								Forgot your username?</a>
							</li>
							<li>
								<a href="#">
									Create an account</a>
							</li>
						</ul>
					</form>
					<%
				
				%>
			</div>
		</div>
	</div>
</div>

		
									
							